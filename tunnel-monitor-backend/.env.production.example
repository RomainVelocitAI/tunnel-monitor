# Node Environment
NODE_ENV=production
PORT=3001

# Frontend URL (for CORS) - IMPORTANT: Set this on Render
FRONTEND_URL=https://tunnel-monitor.vercel.app

# Airtable Configuration - IMPORTANT: Set these on Render
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=appwwQ9gntk3uxRlD
AIRTABLE_TABLE_NAME=URL A SURVEILLER
AIRTABLE_HISTORY_TABLE=Historique_Tests_Tunnels

# GitHub Token for Actions - IMPORTANT: Set this on Render
GITHUB_TOKEN=your_github_token

# Webhook Configuration (optional)
N8N_WEBHOOK_URL=
N8N_WEBHOOK_KEY=

# Monitoring Configuration
ENABLE_MONITORING=true
MONITORING_INTERVAL=0 8 * * *
TEST_TIMEOUT=30000

# Logging
LOG_LEVEL=info
LOG_DIR=logs