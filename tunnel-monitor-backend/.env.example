# Node Environment
NODE_ENV=production
PORT=3001

# Frontend URL (for CORS)
FRONTEND_URL=https://your-frontend.vercel.app

# Airtable Configuration
AIRTABLE_API_KEY=pat_your_personal_access_token
AIRTABLE_BASE_ID=appXXXXXXXXXXXX
AIRTABLE_TABLE_NAME=URL_A_SURVEILLER
AIRTABLE_HISTORY_TABLE=Historique_Tests_Tunnels

# Webhook Configuration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/tunnel-monitoring
N8N_WEBHOOK_KEY=optional_security_key

# Monitoring Configuration
ENABLE_MONITORING=true
MONITORING_INTERVAL=0 8 * * *
TEST_TIMEOUT=30000

# Logging
LOG_LEVEL=info
LOG_DIR=logs